%generate map [Sll[readonly]] Sll

data List := Nil | Cons Int List;

Sll : List >-> layout[x];
Sll (Nil) := emp;
Sll (Cons head tail) := x :-> head, (x+1) :-> tail, Sll tail;

map : List -> List;
map (Nil) := Nil;
map (Cons x xs) := Cons (instantiate [Int] Int add1 x) (map xs);

add1 : Int -> Int;
add1 x := x + 1;

